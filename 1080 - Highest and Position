#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    int n,maxVal=0,position=0;
    for (int i=1;i<=100;i++){
        cin>>n;
        if(n>maxVal){
            maxVal=n;    
            position=i;    
        }
    }
    cout<<maxVal<<endl;
    cout<<position<<endl;
    return 0;
}
