#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main(){
    double a,b,c,R1,R2,delta;
    cin>>a>>b>>c;
    delta=(b*b-4*a*c);
    if(a==0 || delta<0){
        cout<<"Impossivel calcular"<<endl;
    }else{
    R1=(-b+sqrt(delta))/(2*a);
    R2=(-b-sqrt(delta))/(2*a);
    cout<<fixed<<setprecision(5)<<"R1 = "<<R1<<endl;
    cout<<fixed<<setprecision(5)<<"R2 = "<<R2<<endl;
    }
    return 0;
}
