#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
    int X;
    while (true){
        cin>>X;
        if(X == 0){
            break;
        }
        for(int i=1;i<=X;i++){
            cout<<i;
            if(i<X){
                cout<<" ";
            }
        }
        
        cout<<endl; 
    }
    
    return 0;
}
