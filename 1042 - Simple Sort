#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cin>>a>>b>>c;
    int original_order[3]={a,b,c};
    int sorted_order[3]={a,b,c};
    sort(sorted_order,sorted_order+3);
    for(int i=0;i<3;i++){
        cout<<sorted_order[i]<<endl;
    }
    cout<<endl;
    for(int i=0;i<3;i++){
        cout<<original_order[i]<<endl;
    }
    return 0;
}
